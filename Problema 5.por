programa {
  funcao inicio() {
    //Declaração de Variáveis
      real altura, peso_ideal

    //Declaração de constante
      const real k=72.7
      const inteiro b=58

    //Entrada de dados
      escreva("informe a altura ")
      leia(altura)

    //Processamento
    peso_ideal= k*altura-b

    //Saida de dados
    escreva("O peso ideal é: ", peso_ideal)

  }
}
